#ERROR if repeated integers
#ERROR if beyond limits
#RROR if float/double
all:
	make -C $(LIBFT)/
	gcc $(FLAG2) *.c algos/*.c cmds/*c $(LIBFT)/$(LIBFT).a -o $(NAME)
clean:
	make clean -C $(LIBFT)/
fclean: clean
	rm -f $(NAME)
	make fclean -C $(LIBFT)/
re: fclean all
t1: all
	./$(NAME) 12114 -32312 1000
t2: all
	./$(NAME) 4 3 2 1 0
t3: all
	./$(NAME) 2 1 3 6 5 -8 123 441 -564 13
t4: all
	./$(NAME) 1 2 3 4 5
c1: all
	./$(NAME) $(LARGE) | ./$(CHECKER) $(LARGE)
c2: all
	./$(NAME) $(VLARGE) | ./$(CHECKER) $(VLARGE)
.PHONY: $(NAME) all clean fclean re t1 t2 t3 t4 c1 c2
